{
  "id": "plugin-amazon-aurora",
  "displayName": "Amazon Aurora",
  "description": "Publisher which connects to Amazon Aurora",
  "apiVersion": "2",
  "versionDescription": "Version 2",
  "version": "1.0.0",
  "iconFile": "icon.png",
  "executable": "PluginAmazonAurora",
  "kind": "publisher",
  "os": "$GOOS",
  "arch": "$GOARCH",
  "accessGroups": [
    "vandelay",
    "prodsupport",
    "populytics",
    "wyatt",
    "casey",
    "katie",
    "waynedemo"
  ],
  "canProduceMultipleSchemas": true,
  "canAcceptQueryBasedSchemas": false,
  "canUseOAuth": false,
  "canWrite": false,
  "canConfigureReplication": true,
  "canWriteReplication": true,
  "querySchema": {
    "ui": {
      "ui:widget": "code",
      "ui:options": {
        "language": "sql"
      }
    },
    "schema": {
      "type": "string"
    }
  },
  "configSchema": {
    "ui": {
      "ui:order": ["Hostname", "Port", "Database", "Username", "Password"],
      "Password": {
        "ui:widget": "password"
      }
    },
    "schema": {
      "type": "object",
      "properties": {
        "Hostname": {
          "type": "string",
          "title": "Hostname",
          "description": "The hostname of the Amazon Aurora Server"
        },
        "Port": {
          "type": "string",
          "title": "Port",
          "description": "The port of the Amazon Aurora Server"
        },
        "Username": {
          "type": "string",
          "title": "Username",
          "description": "The username to use for authentication"
        },
        "Password": {
          "type": "string",
          "title": "Password",
          "description": "The password to use for authentication"
        },
        "Database": {
          "type": "string",
          "title": "Database",
          "description": "The Database within the Amazon Aurora Server"
        }
      },
      "required": ["Hostname", "Username", "Password", "Database"]
    }
  }
}
